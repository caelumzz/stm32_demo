    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "request": "launch",
            "type": "cortex-debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/${workspaceFolderBasename}.elf",
            "servertype": "openocd",
            "device": "STM32F103C8Tx",
            "interface": "swd",
            "configFiles": [
                "${workspaceRoot}/openocd.cfg"
            ],
            "searchDir": [],
            "runToMain": true,
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none"
        }

    ]
}